COL		= $(COLOR_OBJS)
SHM		= $(XSHM_OBJS)
DBE		= $(XDBE_OBJS)
BARS		= $(UTILS_BIN)/colorbars.o $(LOGO)
ATV             = analogtv.o $(SHM)
APPLE2          = apple2.o $(ATV)

