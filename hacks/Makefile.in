check_men:
	@badmen="" ;							\
	 for exe in $(EXES) $(SCRIPTS); do				\
	   if ! [ -f $(srcdir)/$$exe.man				\
		  -o "$$exe" = webcollage-helper ]; then		\
	     badmen="$$badmen $$exe" ;					\
	   fi ;								\
	 done ;								\
	 if [ -n "$$badmen" ]; then					\
	   echo "" ;							\
	   echo "Warning: The following programs have no manuals:" ;	\
	   echo "" ;							\
	   for m in $$badmen ; do					\
	     echo "    $$m" ;						\
	   done ;							\
	 fi

