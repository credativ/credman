xscreensaver-sgigl: xscreensaver-sgigl.c
	$(CC) $(LDFLAGS) -o $@ $< -I$(UTILS_SRC) $(HACK_PRE) \
		$(XMU_LIBS) -lX11 -lXext $(X_EXTRA_LIBS) -lm


