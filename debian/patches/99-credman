+++ b/hacks/images/pacman/convert.sh
@@ -0,0 +1,17 @@
+#!/bin/sh
+
+convert -scale 400%             cd0.xpm    credman-ra.xpm
+convert -scale 400%             cd0-r1.xpm credman-rb.xpm
+convert -scale 400%             cd0-r2.xpm credman-rc.xpm
+
+convert -scale 400% -rotate -90 cd0.xpm    credman-ua.xpm
+convert -scale 400% -rotate -90 cd0-r1.xpm credman-ub.xpm
+convert -scale 400% -rotate -90 cd0-r2.xpm credman-uc.xpm
+
+convert -scale 400% -rotate 90  cd0.xpm    credman-da.xpm
+convert -scale 400% -rotate 90  cd0-r1.xpm credman-db.xpm
+convert -scale 400% -rotate 90  cd0-r2.xpm credman-dc.xpm
+
+convert -scale 400% -rotate 180 cd0.xpm    credman-la.xpm
+convert -scale 400% -rotate 180 cd0-r1.xpm credman-lb.xpm
+convert -scale 400% -rotate 180 cd0-r2.xpm credman-lc.xpm
--- /dev/null
